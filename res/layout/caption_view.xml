<?xml version="1.0" encoding="utf-8"?>
<GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:columnCount="3"
    android:orientation="vertical"
    android:rowCount="13" >

    <TextView
        android:id="@+id/ultrason_label"
        android:layout_width="129dp"
        android:layout_column="1"
        android:layout_gravity="left"
        android:layout_row="1"
        android:text="Ultrason"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <Space
        android:layout_width="21dp"
        android:layout_height="1dp"
        android:layout_column="0"
        android:layout_row="0" />

    <Space
        android:layout_width="1dp"
        android:layout_height="21dp"
        android:layout_column="0"
        android:layout_row="0" />

    <Space
        android:layout_width="170dp"
        android:layout_height="1dp"
        android:layout_column="1"
        android:layout_row="0" />

    <TextView
        android:id="@+id/ultrason_value_0"
        android:layout_column="2"
        android:layout_gravity="left|bottom"
        android:layout_row="1"
        android:text="0"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <Space
        android:layout_width="128dp"
        android:layout_height="15dp"
        android:layout_column="2"
        android:layout_row="1" />

    <Space
        android:layout_width="1dp"
        android:layout_height="37dp"
        android:layout_column="0"
        android:layout_row="2" />

    <Space
        android:layout_width="1dp"
        android:layout_height="43dp"
        android:layout_column="0"
        android:layout_row="5" />

    <Space
        android:layout_width="100dp"
        android:layout_height="332dp"
        android:layout_column="2"
        android:layout_row="9" />

    <Space
        android:layout_width="100dp"
        android:layout_height="332dp"
        android:layout_column="2"
        android:layout_row="9" />

    <Space
        android:layout_width="1dp"
        android:layout_height="15dp" />

    <TextView
        android:id="@+id/ultrason_value_1"
        android:layout_column="2"
        android:layout_gravity="left|center_vertical"
        android:layout_row="2"
        android:text="0"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/inductif_label"
        android:layout_width="88dp"
        android:layout_column="1"
        android:layout_gravity="left|top"
        android:layout_row="6"
        android:text="Inductif"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/inductif_value_0"
        android:layout_column="2"
        android:layout_gravity="left|top"
        android:layout_row="6"
        android:text="0"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/inductif_value_2"
        android:layout_column="2"
        android:layout_gravity="left|top"
        android:layout_row="9"
        android:text="0"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/inductif_value_1"
        android:layout_column="2"
        android:layout_gravity="left|top"
        android:layout_row="8"
        android:text="0"
        android:textAppearance="?android:attr/textAppearanceLarge" />

</GridLayout>